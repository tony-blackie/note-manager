    "version": "1.0.0",
    "description": "App for form generation",
    "main": "app.tsx",
    "engines": {
        "node": "6.9.3"
    },
    "jest": {
        "moduleFileExtensions": ["ts", "tsx", "js"],
        "transform": {
            "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
    },
    "scripts": {
        "start": "npm run build:dev",
        "build": "node_modules/.bin/webpack --config ./webpack/webpack.prod.js",
        "build:dev":
            "node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.js",
        "build:accessible":
            "node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.js --host 0.0.0.0 --disable-host-check",
        "postinstall": "npm run build",
        "test": "jest",
        "coverage": "node_modules/.bin/jest --coverage"
    },
